{
  "name": "contentpull",
  "description": "A contentful.js wrapper that adds simply sugar functions and the ability to parse responses.",
  "version": "0.0.0-semantically-released",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run lint && npm t",
    "commit": "git-cz",
    "lint": "standard",
    "predoc": "rimraf docs",
    "doc": "jsdoc -c doc.json",
    "test": "npm run build && nyc _mocha --compilers js:babel-register",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist src",
    "coverage": "nyc report --reporter=lcov",
    "travis": "npm run precommit && npm run coverage",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remedyhealth/contentpull.git"
  },
  "keywords": [
    "contentpull",
    "contentpull.js",
    "contentpul",
    "contentpul.js",
    "contentful",
    "contentful.js",
    "CDA",
    "parser",
    "reader",
    "promise"
  ],
  "author": "Remedy Health Media <support@remedyhealthmedia.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remedyhealth/contentpull/issues"
  },
  "homepage": "https://github.com/remedyhealth/contentpull#readme",
  "dependencies": {
    "contentful": "5.1.3",
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "8.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.1",
    "chai-subset": "^1.5.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc": "^3.5.5",
    "mitm": "^1.3.2",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.1",
    "rimraf": "^2.6.1",
    "semantic-release": "^12.2.2",
    "standard": "^10.0.2"
  },
  "files": [
    "dist",
    "browser.js",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "browser": "./browser.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  }
}
